{% extends 'Admin/base.html.twig' %}
{% set page_title="Member" %}
{% block body %}
<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email
        </th>
        <th>
            Edit
        </th>
    </tr>
    <tr>
        <td>
            {{ member.firstName }}<br />
        </td>
        <td>
            {{ member.lastName }}<br />
        </td>
        <td>
            {{ member.email }}<br />
        </td>
        <td>
            <a class="btn btn-default" href="{{ path('app_admin_member_edit', {'id': member.id}) }}">Edit</a>
        </td>
    </tr>
</table>
<a href="{{ path('app_admin_member_list_all') }}">Back to the list</a>
{% endblock %}
{% block stylesheets %}
{% endblock %}
